{
  "expo": {
    "name": "wtlfo",
    "slug": "wtlfo",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "wtlfo",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#000000",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.brents.wtlfo",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "appleTeamId": "GQ4S96SE9Z"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#000000",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.brents.wtlfo"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ],
      [
        "expo-build-properties",
        {
          "buildReactNativeFromSource": true,
          "useHermesV1": true
        }
      ],
      "expo-sqlite",
      "expo-font",
      "expo-image",
      "expo-web-browser",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "wtlfo",
          "organization": "brent-projects",
          "dsn": "https://1397d1c25ba952f620723abd186c27ac@o85374.ingest.us.sentry.io/4510763027464192"
        }
      ],
      ["react-native-audio-api", { "iosBackgroundMode": false }]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "fd7017b3-0e29-4b90-8a09-cfdb437daca5"
      }
    },
    "owner": "brents",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "enabled": true,
      "url": "https://u.expo.dev/fd7017b3-0e29-4b90-8a09-cfdb437daca5",
      "enableBsdiffPatchSupport": true,
      "requestHeaders": {
        "expo-channel-name": "main"
      }
    }
  }
}
